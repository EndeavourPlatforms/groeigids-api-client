{
    "name": "endeavour/groeigids-api-client",
    "description": "Api client for importing GroeiGids articles",
    "type": "library",
    "config": {
        "platform": {
            "php": "8.1.999"
        }
    },
    "require": {
        "php": "8.1.*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Guzzle is a PHP HTTP client that implements PSR-18 (suggested implementation)"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Endeavour\\GroeigidsApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavour\\GroeigidsApiClient\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "J. Oonk"
        }
    ]
}
